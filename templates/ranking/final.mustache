<div class="container-fluid ranking-content general-ranking final-ranking {{#isteacher}}isteacher{{/isteacher}}" data-sid="{{sessionid}}" data-cmid="{{cmid}}" data-jqshowid="{{jqshowid}}" data-region="ranking-content">
    <div class="row">
        <div class="col-12 ranking-container">
            <div class="ranking-heading">
                <h2>{{#str}}final_ranking, mod_jqshow{{/str}}</h2>
                <div class="question-fullscreen" data-action="question-fullscreen">
                    {{#pix}}q/zoomin, mod_jqshow{{/pix}}
                </div>
                <div class="question-fullscreen" data-action="question-exit-fullscreen">
                    {{#pix}}q/zoomout, mod_jqshow{{/pix}}
                </div>
            </div>
            <div class="ranking-users finalranking container-fluid px-0" data-region="ranking-users">
                {{> mod_jqshow/ranking/rankingfirstthree }}
                <div class="ranking-users-body" data-region="ranking_users">
                    <div class="content-users">
                        <div class="d-table list-group">
                            {{#finalranking}}
                                <div class=" FINAL d-table-row ranking-users-data user-item" data-userid="{{userid}}">
                                    <div class="d-table-cell userposition text-center">
                                        <span>{{userposition}}</span>
                                    </div>
                                    <div class="d-table-cell username">
                                        <img src="{{userimageurl}}" alt="{{userfullname}}" /> <span>{{userfullname}}</span>
                                    </div>
                                    <div class="d-table-cell userpoints text-center">
                                        <span>{{userpoints}}</span>
                                        {{#pix}}q/points, mod_jqshow{{/pix}}
                                    </div>
                                </div>
                            {{/finalranking}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{#programmedmode}}
    <a class="btn btn-primary m-3 float-right" href="{{reporturl}}">
        {{#str}}report, mod_jqshow{{/str}}
    </a>
{{/programmedmode}}
{{#js}}
    require(['mod_jqshow/question'], function(Question) {
        Question.initQuestion('[data-region="ranking-content"]');
    });
{{/js}}
