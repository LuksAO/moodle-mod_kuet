<div class="row student-waiting-room">
    <div class="col-12">
        <div class="content-waiting-room">
            <div class="waiting-room-title">
                <h2>{{#str}}waitingroom_message, mod_kuet{{/str}}</h2>
            </div>
            <div class="waiting-room-participants">
                <div class="waiting-room-subtitle">
                    {{#isgroupmode}}
                        <h5>{{#pix}}s/groups, mod_kuet{{/pix}} {{#str}}ready_groups, mod_kuet{{/str}}</h5>
                    {{/isgroupmode}}
                    {{^isgroupmode}}
                        <h5>{{#pix}}s/users, mod_kuet{{/pix}} {{#str}}ready_users, mod_kuet{{/str}}</h5>
                    {{/isgroupmode}}
                </div>
                <div class="participants" data-region="active-users">

                </div>
            </div>
        </div>
    </div>
</div>
