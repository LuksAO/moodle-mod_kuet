<div class="row content-header">
    <div class="more-info-session col-12">
        <div class="sessionname">
            {{#pix}}s/name, mod_jqshow{{/pix}}
            <div class="sessionname-text">
                {{#str}}session_name_placeholder, mod_jqshow{{/str}}
                <span>{{sessionname}}</span>
            </div>
        </div>
        <div class="numusers">
            {{#pix}}s/users, mod_jqshow{{/pix}}
            <div class="countusers">
                <span id="countusers">0</span>
                <span class="participants">{{#str}}participants, mod_jqshow{{/str}}</span>
            </div>
        </div>
    </div>
</div>
<div class="row student-waiting-room">
    <div class="col-12">
        <div class="content-waiting-room">
            <div class="waiting-room-title">
                <h2>{{#str}}waitingroom_message, mod_jqshow{{/str}}</h2>
            </div>
            <div class="waiting-room-participants">
                <div class="waiting-room-subtitle">
                    <h5>{{#pix}}s/users, mod_jqshow{{/pix}} {{#str}}ready_users, mod_jqshow{{/str}}</h5>
                </div>
                <div class="participants" data-region="active-users">

                </div>
            </div>
        </div>
    </div>
</div>
