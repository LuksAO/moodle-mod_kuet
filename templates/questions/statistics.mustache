{{!
    @template mod_kuet/statistics

    Statistics

    Classes required for JS:
    * None

    Data attibutes required for JS:
    * None

    Context variables required for this template:
    * none

    Example context (json):
    {
    }
}}
<div id="statistics">
    <div class="onestatistic">
        <span>
            {{#pix}}q/correct, mod_kuet{{/pix}}
            {{#str}}correct, mod_kuet{{/str}}
        </span>
        <div class="progress">
            <div class="progress-bar correct" role="progressbar" aria-valuenow="" aria-valuemin="0" aria-valuemax="100">{{ correct }}</div>
        </div>
    </div>
    <div class="onestatistic">
        <span>
            {{#pix}}q/partially, mod_kuet{{/pix}}
            {{#str}}partially, mod_kuet{{/str}}
        </span>
        <div class="progress">
            <div class="progress-bar partially" role="progressbar" aria-valuenow="" aria-valuemin="0" aria-valuemax="100">{{ partially }}</div>
        </div>
    </div>
    <div class="onestatistic">
        <span>
            {{#pix}}q/incorrect, mod_kuet{{/pix}}
            {{#str}}incorrect, mod_kuet{{/str}}
        </span>
        <div class="progress">
            <div class="progress-bar failure" role="progressbar" aria-valuenow="" aria-valuemin="0" aria-valuemax="100">{{ failure }}</div>
        </div>
    </div>
    <div class="onestatistic">
        <span>
            {{#pix}}q/noresponse, mod_kuet{{/pix}}
            {{#str}}noresponse, mod_kuet{{/str}}
        </span>
        <div class="progress">
            <div class="progress-bar noresponse" role="progressbar" aria-valuenow="" aria-valuemin="0" aria-valuemax="100">{{ noresponse }}</div>
        </div>
    </div>
</div>