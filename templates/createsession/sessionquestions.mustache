<div class="questions-list-body d-table list-group" data-region="question_list" data-sid="{{sid}}" data-cmid="{{cmid}}" data-jqshowid="{{jqshowid}}">
    <div class="d-table-row questions-list-thead">
        {{^ispage3}}
            {{^waitingroom}}
                <div class="d-table-cell"></div>
            {{/waitingroom}}
        {{/ispage3}}
        <div class="d-table-cell text-center">{{#str}}question_position, mod_jqshow{{/str}}</div>
        <div class="d-table-cell">{{#str}}question_name, mod_jqshow{{/str}}</div>
        <div class="d-table-cell text-center">{{#str}}question_type, mod_jqshow{{/str}}</div>
        {{^ispage1}}
            <div class="d-table-cell text-center">{{#str}}question_time, mod_jqshow{{/str}}</div>
        {{/ispage1}}
        <div class="d-table-cell text-center">{{#str}}question_version, mod_jqshow{{/str}}</div>
        <div class="d-table-cell text-center">{{#str}}question_actions, mod_jqshow{{/str}}</div>
    </div>
    {{#sessionquestions}}
        <div class="d-table-row questions-list-data question-item" data-questionnid="{{questionnid}}">
            {{^ispage3}}
                {{^waitingroom}}
                    <div class="d-table-cell">
                        <span tabindex="0" role="button" aria-haspopup="false" data-drag-type="move">
                            {{#pix}}i/dragger, mod_jqshow{{/pix}}
                        </span>
                    </div>
                {{/waitingroom}}
            {{/ispage3}}
            <div class="d-table-cell text-center">{{position}}</div>
            <div class="d-table-cell">{{name}}</div>
            <div class="d-table-cell text-center">
                {{#icon}}
                    {{#pix}} {{key}}, {{component}}, {{title}} {{/pix}}
                {{/icon}}
            </div>
            {{^ispage1}}
                <div class="d-table-cell text-center">{{time}}</div>
            {{/ispage1}}
            <div class="d-table-cell text-center">{{version}} </div>
            <div class="d-table-cell actions">
                <div class="content-actions">
                    {{^ispage3}}
                        {{#managesessions}}
                            <a href="{{editquestionurl}}" class="question-action">
                                {{#pix}} i/edit_session, mod_jqshow {{/pix}}
                            </a>
                            <div class="question-action" data-action="preview_question" data-questionnid="{{questionnid}}">
                                <div class="question-action" data-action="question_preview" data-questionnid="{{questionnid}}">
                                    {{#pix}} i/preview, mod_jqshow {{/pix}}
                                </div>
                            </div>
                            <div class="question-action" data-action="delete_question" data-questionnid="{{questionnid}}">
                                {{#pix}} i/delete_session, mod_jqshow {{/pix}}
                            </div>
                        {{/managesessions}}
                    {{/ispage3}}
                    {{#ispage3}}
                        <div class="question-action" data-action="question_preview" data-questionnid="{{questionnid}}">
                            {{#pix}} i/preview, mod_jqshow {{/pix}}
                        </div>
                    {{/ispage3}}
                    {{#waitingroom}}
                        <div class="question-action" data-action="question_preview" data-questionnid="{{questionnid}}">
                            {{#pix}} i/preview, mod_jqshow {{/pix}}
                        </div>
                    {{/waitingroom}}
                </div>
            </div>
        </div>
    {{/sessionquestions}}
</div>
{{#js}}
    require(['mod_jqshow/sessionquestions'],
        function(SessionQuestions) {
            SessionQuestions.initSessionQuestions('[data-region="question_list"]');
        }
    );
{{/js}}
