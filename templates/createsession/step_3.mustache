<div class="content-header">
    <div class="content-title">
        <h2>{{#str}}summarysession, mod_kuet{{/str}}</h2>
        <small>{{#str}}summarysession_info, mod_kuet{{/str}}</small>
    </div>
    <div class="session-steps">
        <div class="step step1">
            <div class="timeline timeline-r"></div>
            <span class="step-text"></span>
        </div>
        <div class="step step2">
            <div class="timeline timeline-r"></div>
            <div class="timeline timeline-l"></div>
            <span class="step-text"></span>
        </div>
        <div class="step step3 current">
            <div class="timeline timeline-l"></div>
            <span class="step-text bg-primary">3</span>
        </div>
    </div>
</div>
<div class="row summary-session">
    <div class="col-12 col-lg-5 container">
        <div class="resume-config">
            <h6 class="titlecontainer bg-primary">
                {{#pix}} i/settings, mod_kuet {{/pix}}
                {{#str}}configuration, mod_kuet{{/str}}
            </h6>
            {{> mod_kuet/session/sessionresume }}
        </div>
    </div>
    <div class="col-12 col-lg-7 container">
        <div class="questions-list">
            <h6 class="titlecontainer bg-primary">
                {{#pix}} i/questions_list, mod_kuet {{/pix}}
                {{#str}}questions_list, mod_kuet{{/str}}
            </h6>
            <div class="session-questions" data-region="session-questions">
                {{> mod_kuet/createsession/sessionquestions }}
            </div>
        </div>
    </div>
    <div class="col-12">
        <hr>
        <div class="d-flex flex-nowrap align-items-center justify-content-center">
            <div class="form-group fitem btn-cancel mr-3">
            <span data-fieldtype="submit">
                <a href="{{addquestions}}" class="btn btn-secondary">
                    {{#str}}previous, moodle{{/str}}
                </a>
            </span>
            </div>
            <div class="form-group fitem">
            <span data-fieldtype="submit">
                <a href="{{sessionsurl}}" class="btn btn-primary disabled" data-action="end_create_session">
                    {{#str}}end, mod_kuet{{/str}}
                </a>
            </span>
            </div>
        </div>
    </div>
</div>
